{
  "manifest_version": 3,
  "name": "Gitlab Swimlanes Chrome Plugin",
  "version": "1.0",
  "description": "Modifica la vista de GitLab mostrando un tablero personalizado.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://gitlab.arba.gov.ar/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "all_frames": true
    }
  ]
}
